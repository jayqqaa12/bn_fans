{
  "name": "bn-fans",
  "version": "1.0.0",
  "description": "币安自动关注机器人",
  "author": "Developer",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac"
  },
  "dependencies": {
    "express": "^5.2.1",
    "https-proxy-agent": "^7.0.6",
    "proxy-agent": "^6.5.0",
    "puppeteer": "^23.0.0",
    "socks-proxy-agent": "^8.0.5"
  },
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.bn-fans.app",
    "productName": "BinanceFollower",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "index.html",
      "package.json"
    ],
    "mac": {
      "target": ["dmg"]
    },
    "win": {
      "target": ["nsis"]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "puppeteer": {
    "skipDownload": true
  }
}
